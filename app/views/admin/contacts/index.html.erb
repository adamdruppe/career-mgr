<h1>Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Address 1</th>
      <th>Address 2</th>
      <th>City</th>
      <th>State</th>
      <th>Postal code</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Url</th>
      <th>Contactable</th>
      <th>Contactable type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.address_1 %></td>
        <td><%= contact.address_2 %></td>
        <td><%= contact.city %></td>
        <td><%= contact.state %></td>
        <td><%= contact.postal_code %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.email %></td>
        <td><%= contact.url %></td>
        <td><%= contact.contactable_id %></td>
        <td><%= contact.contactable_type %></td>
        <td><%= link_to 'Show', admin_contact_path(contact) %></td>
        <td><%= link_to 'Edit', edit_admin_contact_path(contact) %></td>
        <td><%= link_to 'Destroy', admin_contact_path(contact), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact', new_admin_contact_path %>
