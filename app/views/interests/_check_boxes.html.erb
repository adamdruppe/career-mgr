<% object_name = object.class.name.downcase %>  

<p>
  Interests<br>
  <% Interest.all.each do |interest| %>
    <%= check_box_tag("#{object_name}[interest_ids][]", interest.id, object.interests.include?(interest)) %>
    <%= label_tag("#{object_name}[interest_ids][]", interest.name, style: 'display: inline;') %><br>
  <% end %>
</p>
